DROP TABLE IF EXISTS User ;
CREATE TABLE User (id_user_User INT AUTO_INCREMENT NOT NULL,
mail_User VARCHAR,
name_User VARCHAR,
mdp_User VARCHAR(255),
PRIMARY KEY (id_user_User)) ENGINE=InnoDB;

DROP TABLE IF EXISTS Score ;
CREATE TABLE Score (id_score_Score INT AUTO_INCREMENT NOT NULL,
score_Score INT,
date_Score DATE,
id_user_User **NOT FOUND**,
PRIMARY KEY (id_score_Score)) ENGINE=InnoDB;

DROP TABLE IF EXISTS droit ;
CREATE TABLE droit (id_droit INT AUTO_INCREMENT NOT NULL,
name_droit VARCHAR,
roles_droit ENUM,
PRIMARY KEY (id_droit)) ENGINE=InnoDB;

ALTER TABLE Score ADD CONSTRAINT FK_Score_id_user_User FOREIGN KEY (id_user_User) REFERENCES User (id_user_User);

